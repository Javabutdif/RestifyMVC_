@model IEnumerable<Restify.Models.Landlord>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.landlord_firstname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.landlord_lastname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.landlord_email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.landlord_contact)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.landlord_firstname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.landlord_lastname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.landlord_email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.landlord_contact)
            </td>
 
            <td>
                <a asp-action="Edit" asp-route-id="@item.landlord_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.landlord_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.landlord_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
